    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^30.1.2",
    "detox": "^20.41.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
